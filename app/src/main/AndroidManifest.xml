        <activity android:name=".timeline_view.TimelineViewActivity" />
        <activity android:name=".stack_text_view.StackTextActivity" />
        <activity android:name=".circular_image_view.CircularImageActivity" />
        <activity android:name=".stack_image_view.StackImageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>